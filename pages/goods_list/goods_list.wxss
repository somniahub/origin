navigator {
  display: flex;
  padding: 10rpx 0;
  border-bottom: 2rpx solid #ccc;
  height: 220rpx;
}
navigator .goods_image {
  flex: 3;
  display: flex;
  justify-content: center;
  align-items: center;
}
navigator .goods_image image {
  width: 65%;
  height: 65%;
}
navigator .goods_info {
  flex: 5;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-top: 15rpx;
}
navigator .goods_info .info_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 28rpx;
}
navigator .goods_info .info_price {
  color: var(--themeColor);
  font-size: 32rpx;
  padding-bottom: 15rpx;
}
